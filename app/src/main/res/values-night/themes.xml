<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MyGithubUser" parent="Theme.Material3.DayNight">
        <item name="colorPrimary">@color/mygithub_theme_dark_primary</item>
        <item name="colorOnPrimary">@color/mygithub_theme_dark_onPrimary</item>
        <item name="colorPrimaryContainer">@color/mygithub_theme_dark_primaryContainer</item>
        <item name="colorOnPrimaryContainer">@color/mygithub_theme_dark_onPrimaryContainer</item>
        <item name="colorSecondary">@color/mygithub_theme_dark_secondary</item>
        <item name="colorOnSecondary">@color/mygithub_theme_dark_onSecondary</item>
        <item name="colorSecondaryContainer">@color/mygithub_theme_dark_secondaryContainer</item>
        <item name="colorOnSecondaryContainer">@color/mygithub_theme_dark_onSecondaryContainer</item>
        <item name="colorTertiary">@color/mygithub_theme_dark_tertiary</item>
        <item name="colorOnTertiary">@color/mygithub_theme_dark_onTertiary</item>
        <item name="colorTertiaryContainer">@color/mygithub_theme_dark_tertiaryContainer</item>
        <item name="colorOnTertiaryContainer">@color/mygithub_theme_dark_onTertiaryContainer</item>
        <item name="colorError">@color/mygithub_theme_dark_error</item>
        <item name="colorErrorContainer">@color/mygithub_theme_dark_errorContainer</item>
        <item name="colorOnError">@color/mygithub_theme_dark_onError</item>
        <item name="colorOnErrorContainer">@color/mygithub_theme_dark_onErrorContainer</item>
        <item name="android:colorBackground">@color/mygithub_theme_dark_background</item>
        <item name="colorOnBackground">@color/mygithub_theme_dark_onBackground</item>
        <item name="colorSurface">@color/mygithub_theme_dark_surface</item>
        <item name="colorOnSurface">@color/mygithub_theme_dark_onSurface</item>
        <item name="colorSurfaceVariant">@color/mygithub_theme_dark_surfaceVariant</item>
        <item name="colorOnSurfaceVariant">@color/mygithub_theme_dark_onSurfaceVariant</item>
        <item name="colorOutline">@color/mygithub_theme_dark_outline</item>
        <item name="colorOnSurfaceInverse">@color/mygithub_theme_dark_inverseOnSurface</item>
        <item name="colorSurfaceInverse">@color/mygithub_theme_dark_inverseSurface</item>
        <item name="colorPrimaryInverse">@color/mygithub_theme_dark_inversePrimary</item>
    </style>

    <style name="circleImageViewStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>